{
	"name": "pathcrafter-monorepo",
	"private": true,
	"scripts": {
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"build": "turbo run build --filter=pathcrafter",
		"version:patch": "turbo run patch --filter=pathcrafter",
		"version:minor": "turbo run minor --filter=pathcrafter",
		"version:major": "turbo run major --filter=pathcrafter",
		"upload": "turbo run publish --filter=pathcrafter",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"af-prettierrc": "^1.0.0",
		"eslint": "^7.32.0",
		"eslint-config-af-typescript": "^1.6.0",
		"prettier": "^2.8.8",
		"turbo": "^1.10.1",
		"typescript": "^5.1.3"
	},
	"packageManager": "pnpm@7.15.0",
	"engines": {
		"node": ">=18.0.0"
	}
}
